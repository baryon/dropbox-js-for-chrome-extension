{
  "name": "dropbox.js example extension",
  "version": "0.3",
  "manifest_version": 2,
  "description": "Example of Dropbox JS for Chrome Extension.",
  "app": { "launch": { "local_path": "example.html" } },
  "permissions": [
    "unlimitedStorage",
    "tabs",
    "https://www.dropbox.com/",
    "https://api.dropbox.com/",
    "https://api-content.dropbox.com/"
  ]
}
